              description="OLE DB Source"
              name="MUNIS DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SET NOCOUNT ON

SELECT 
	replace(prim.a_name_first + ' ' + prim.a_name_minit + ' ' + prim.a_name_last, '  ', ' ') AS 'Name',
	prim.a_name_first AS 'Preferred Name',
	'' AS 'Employee Id',
	prim.e_email AS 'Email',
	format(prim.e_date_of_birth, 'MMM dd yyyy') 'Date of Birth',
	format(prim.e_orig_hire_date, 'MMM dd yyyy') AS 'Start Date',
	ISNULL(format(prim.e_terminated_date, 'MMM dd yyyy'), '') AS 'End Date',
	'en' as 'Language',
	man.e_email as 'Manager Email',
	replace(man.a_name_first + ' ' + man.a_name_minit + ' ' + man.a_name_last, '  ', ' ') as 'Manager',
	eval.e_overall_rating as 'Performance Rating',
	prim.e_gender as 'Gender',
	'OCCC' as 'Location',
	prim.a_location_p_desc as 'Department',
	case
		when prim.e_eeo_part_full_t = 'F' then 'Full-time' 
		when prim.e_eeo_part_full_t = 'P' then 'Part-time' 
	end as 'Employment Type',
	prim.a_job_class_desc as 'Job Title'

FROM pr_employee_master prim
	LEFT JOIN pr_employee_master man
	on prim.e_supervisor = man.a_employee_number
	LEFT JOIN (
				select pm_evaluations.*, row_number() over (partition by a_employee_number order by e_actual_cpl_date desc) as rn 
				from pm_evaluations 
			) eval  on eval.a_employee_number = prim.a_employee_number
WHERE prim.e_activity_status = 'A'	
and lower(prim.a_name_last) in ('still', 'watts', 'smith') and prim.a_location_primary = 2101
and eval.rn = 1
ORDER BY 'Name'</property>
